<?xml version="1.0"?>
<launch>

	<!-- ////////////////////////////////////////////////////////////////////////////////////////////////////// -->
	<!-- send chair_wicker urdf to param server -->
    	<param name="chair" command="$(find xacro)/xacro.py $(find hbrs_gazebo_objects)/objects/chair_wicker.urdf" />

	<!-- spawn uploaded wall model -->
    	<node name="spawn_chair" pkg="gazebo_ros" type="spawn_model" args="-urdf -param chair  -x 5.0 -y 2.0 -z 0.01 -R 0 -P 0 -Y 0.785 -model chair1" respawn="false" output="screen" />

	
</launch>



