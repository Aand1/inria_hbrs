<?xml version="1.0" ?>
<sdf version="1.4">
  <world name="gazebo">
    <!-- A global light source -->
    <include>
      <uri>model://sun</uri>
    </include>
    <!-- A ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <plugin name="gazebo_ros_objects_state_publisher" filename="libgazebo_ros_objects_state_publisher.so"/>
  </world>
</sdf>

